{
  "name": "prompt-eval-test-harness",
  "version": "1.1.2",
  "description": "LLM Prompt Evaluation Test Harness - Test prompts against labeled datasets",
  "main": "server/index.js",
  "scripts": {
    "build": "npx rimraf dist && node scripts/build.js",
    "start": "node scripts/start-dev.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:all": "npm test && cd client && npm test",
    "clean": "npx rimraf node_modules package-lock.json && npm install",
    "setup-db": "node server/scripts/setup-database.js",
    "prepare": "husky"
  },
  "keywords": [
    "llm",
    "prompt",
    "evaluation",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bedrock-wrapper": "^2.6.2",
    "body-parser": "^2.2.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "vitest": "^3.2.4"
  }
}
